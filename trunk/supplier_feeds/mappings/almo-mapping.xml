<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
    "http://castor.org/mapping.dtd">

<mapping>
	<class name="AlmoCatalogData">
		<map-to xml="data" />
		<field name="productRecordList" type="AlmoProductRecord"
			collection="collection">
			<bind-xml name="product_record" />
		</field>
	</class>

	<class name="AlmoProductRecord">
		<map-to xml="product_record" />
		<field name="brand" type="java.lang.String">
			<bind-xml name="brand" node="element" />
		</field>
		<field name="partNumber" type="java.lang.String">
			<bind-xml name="part_number" node="element" />
		</field>
		<field name="description" type="java.lang.String">
			<bind-xml name="description" node="element" />
		</field>
		<field name="price" type="java.lang.String">
			<bind-xml name="price" node="element" />
		</field>
		<field name="availableAllWhse" type="java.lang.String">
			<bind-xml name="available_all_whse" node="element" />
		</field>
		<field name="whs126Ohio" type="java.lang.String">
			<bind-xml name="whs_126_Ohio" node="element" />
		</field>
		<field name="whs134Philadelphia" type="java.lang.String">
			<bind-xml name="whs_134_Philadelphia" node="element" />
		</field>
		<field name="whs141NorthCarolina" type="java.lang.String">
			<bind-xml name="whs_141_North_Carolina" node="element" />
		</field>
		<field name="whs166California" type="java.lang.String">
			<bind-xml name="whs_166_California" node="element" />
		</field>
		<field name="whs167Harrisburg" type="java.lang.String">
			<bind-xml name="whs_167_Harrisburg" node="element" />
		</field>
		<field name="whs180Minnesota" type="java.lang.String">
			<bind-xml name="whs_180_Minnesota" node="element" />
		</field>
		<field name="whs185Seattle" type="java.lang.String">
			<bind-xml name="whs_185_Seattle" node="element" />
		</field>
		<field name="whs186Denver" type="java.lang.String">
			<bind-xml name="whs_186_Denver" node="element" />
		</field>
		<field name="whs187Dallas" type="java.lang.String">
			<bind-xml name="whs_187_Dallas" node="element" />
		</field>
		<field name="whs191Wisconsin" type="java.lang.String">
			<bind-xml name="whs_191_Wisconsin" node="element" />
		</field>
		<field name="whs195StLouis" type="java.lang.String">
			<bind-xml name="whs_195_St._Louis" node="element" />
		</field>
		<field name="upcCode" type="java.lang.String">
			<bind-xml name="UPC_Code" node="element" />
		</field>
		<field name="upsShip" type="java.lang.String">
			<bind-xml name="UPS_Ship" node="element" />
		</field>
		<field name="weight" type="java.lang.String">
			<bind-xml name="Weight" node="element" />
		</field>
	</class>
</mapping>