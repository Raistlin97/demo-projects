<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">


	<bean id="schedulerJmxContextProvider"
		class="org.etl.mgr.apps.quartz.jmx.SchedulerJmxContextProviderImpl">
		<constructor-arg index="0">
			<value>scheduler.jmx.mbean</value>
		</constructor-arg>	
	</bean>

	<bean id="schedulerJmxContextFactory"
		class="com.xchanging.etl.mgr.core.jmx.SchedulerJmxContextFactory"
		factory-method="getInstance">
		<property name="schedulerJmxContextProvider" ref="schedulerJmxContextProvider"/>
	</bean>

</beans>
